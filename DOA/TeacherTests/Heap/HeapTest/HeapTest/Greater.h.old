#ifndef GREATER_H
#define GREATER_H

#include "Compare.h"

template <typename T>
class Greater: public Compare<T>
{
public:
	Greater();
	~Greater();
	bool CompareValues(T aFirst, T aSecond) override;
};

template <typename T>
Greater<T>::Greater()
{
	
}

template <typename T>
Greater<T>::~Greater()
{
	
}

template <typename T>
bool Greater<T>::CompareValues(T aFirst, T aSecond)
{
	return !(aFirst < aSecond);
}

#endif